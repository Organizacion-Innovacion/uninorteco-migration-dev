{
  "name": "@uninorte/enrollment-utils",
  "version": "0.0.1",
  "description": "Academic enrollment utilities for uninorte UninorteCO features",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./common": {
      "require": "./dist/common.js",
      "import": "./dist/common.js",
      "types": "./dist/common.d.ts"
    },
    "./repositories": {
      "require": "./dist/repositories.js",
      "import": "./dist/repositories.js",
      "types": "./dist/repositories.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "common": [
        "dist/common.d.ts"
      ],
      "repositories": [
        "dist/repositories.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "jest -c ./jest.unit.json --verbose --runInBand",
    "lint": "eslint --ext .ts,.js ./src"
  },
  "keywords": [],
  "author": "uninorte",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@types/jest": "^29.5.3",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "tsup": "^6.5.0",
    "typescript": "^4.6.4"
  }
}