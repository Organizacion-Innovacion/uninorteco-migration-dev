{
  "name": "@uninorte/enrollment-utils",
  "version": "0.0.1",
  "description": "Utilities for functionalities related to academic enrollment.",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "del-cli ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && move-file lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "jest -c ./jest.unit.json --verbose --runInBand"
  },
  "keywords": [],
  "author": "uninorte",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "loglevel": "^1.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "del-cli": "^5.0.0",
    "jest": "^29.6.1",
    "move-file-cli": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  }
}
